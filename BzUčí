let currentYear = 2024;
let currentChild = 'Nina';

function openSection(id){
  document.querySelectorAll('.section').forEach(s=>s.classList.remove('active'));
  document.getElementById(id).classList.add('active');
  if(id === 'archiv'){
    renderTimeline(currentYear);
  }
  if(id === 'deti'){
    renderChildrenYears();
    renderChildrenTimeline();
  }
}

function goHome(){
  document.querySelectorAll('.section').forEach(s=>s.classList.remove('active'));
  document.getElementById('home').classList.add('active');
}

function setYear(year){
  currentYear = year;
  document.querySelectorAll('.year-btn').forEach(b=>b.classList.remove('active'));
  document.querySelectorAll('.year-btn').forEach(btn=>{
    if(btn.textContent.trim()==String(year)){ btn.classList.add('active'); }
  });
  renderTimeline(year);
}

function renderTimeline(year){
  const timeline = document.getElementById('timeline');
  if(!timeline) return;
  const items = memoryData[year] || [];
  if(items.length === 0){
    timeline.innerHTML = '<p style="font-size:13px;color:var(--text-muted);">ZatÃ­m Å¾Ã¡dnÃ© zÃ¡znamy pro tento rok.</p>';
    return;
  }
  timeline.innerHTML = items.map(item => `
    <div class="memory-card">
      <div class="memory-card-header">
        <div class="memory-type">${item.icon} ${item.type}</div>
        <div class="memory-section">${item.section}</div>
      </div>
      <div class="memory-note">${item.note}</div>
    </div>
  `).join('');
}

function setChild(child){
  currentChild = child;
  document.querySelectorAll('.child-btn').forEach(b=>b.classList.remove('active'));
  document.querySelectorAll('.child-btn').forEach(btn=>{
    if(child === 'Vsechny'){
      if(btn.textContent.indexOf('VÅ¡echny') !== -1){ btn.classList.add('active'); }
    } else {
      if(btn.textContent.indexOf(child) !== -1){ btn.classList.add('active'); }
    }
  });
  renderChildrenYears();
  renderChildrenTimeline();
}

function renderChildrenYears(){
  const yearsContainer = document.getElementById('childrenYears');
  yearsContainer.innerHTML = '';
  const data = childrenData[currentChild] || {};
  const years = Object.keys(data).map(y=>parseInt(y,10)).sort((a,b)=>a-b);
  if(years.length === 0){
    yearsContainer.innerHTML = '<p style="font-size:12px;color:var(--text-muted)">ZatÃ­m Å¾Ã¡dnÃ© roky.</p>';
    return;
  }
  years.forEach((year,index)=>{
    const btn = document.createElement('button');
    btn.className = 'child-year-btn' + (index === years.length-1 ? ' active' : '');
    btn.textContent = year + ' Â· ' + (year - 2019) + '. rok DOMOVINY';
    btn.onclick = ()=>{
      document.querySelectorAll('.child-year-btn').forEach(b=>b.classList.remove('active'));
      btn.classList.add('active');
      renderChildrenTimeline(year);
    };
    yearsContainer.appendChild(btn);
  });
}

function renderChildrenTimeline(yearParam){
  const container = document.getElementById('childrenTimeline');
  const data = childrenData[currentChild] || {};
  const years = Object.keys(data).map(y=>parseInt(y,10)).sort((a,b)=>a-b);
  const year = yearParam || (years.length ? years[years.length-1] : null);
  if(!year){
    container.innerHTML = '<p style="font-size:12px;color:var(--text-muted)">ZatÃ­m Å¾Ã¡dnÃ© milnÃ­ky.</p>';
    return;
  }
  const items = data[year] || [];
  if(items.length === 0){
    container.innerHTML = '<p style="font-size:12px;color:var(--text-muted)">V tomto roce nejsou zatÃ­m zapsanÃ© milnÃ­ky.</p>';
    return;
  }
  container.innerHTML = items.map(item => `
    <div class="memory-card">
      <div class="memory-card-header">
        <div class="memory-type">${item.icon} ${item.type}</div>
        <div class="memory-section">${year}${item.age ? ' Â· ' + item.age : ''}</div>
      </div>
      <div class="memory-note">${item.note}</div>
    </div>
  `).join('');
}

document.addEventListener('DOMContentLoaded', () => {
  if(document.getElementById('archiv').classList.contains('active')){
    renderTimeline(currentYear);
  }
});
</script>

